+      s1 <- project( s0, 1, 
+         control=list(rand.err=0,
+                      rand.seed=0,
+                      endemic.mort=1,
+                      sdi.mort=1,
+                      genetic.gains=0))
+ 
+      sums <- rbind( sums, as.data.frame( cbind( sp.sums( s1 )["DF",], age=s1$age ) ) )
+      s0 <- s1
+ }
