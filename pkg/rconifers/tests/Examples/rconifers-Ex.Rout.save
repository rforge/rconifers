> plot( project( sample.smc, 5, control=list(rand.err=1,
+                      rand.seed=615,
+                      endemic.mort=1,
+                      sdi.mort=1,
+                      genetic.gains=0)) )
> 
> 
> 
> ## create a loop and store the projected values
> s0 <- sample.smc
> sums <- as.data.frame( cbind( sp.sums( s0 )["DF",], age=s0$age ) )
